{
  "name": "durandal-gulp-boilerplate",
  "version": "1.0.1",
  "homepage": "https://github.com/mryellow/durandal-gulp-boilerplate",
  "authors": [
    "mryellow <mr-yellow@mr-yellow.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "durandal": "~2.1.0",
    "jquery": "~2.1.3"
  },
  "overrides": {
    "durandal": {
      "main": [
        "**/*.js",
        "**/*.css"
      ]
    },
    "jquery": {
      "main": "jquery.*.js"
    }
  }
}
