{
  "name": "durandal-gulp-boilerplate",
  "version": "1.0.1",
  "homepage": "https://github.com/mryellow/durandal-gulp-boilerplate",
  "authors": [
    "mryellow <mr-yellow@mr-yellow.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "durandal": "~2.1.0",
    "knockout": "~3.2.0"
  },
  "overrides": {
    "durandal": {
      "main": [
        "**/*.js",
        "**/*.css"
      ],
      "dependencies": {
        "jquery": "1.9.1",
        "requirejs": "2.1.11",
        "requirejs-text": "2.0.7",
        "knockout": "3.2.0" // gulp main-bower-files ignores 3.1.0
      }
    },
    "jquery": {
      "main": "jquery.*.js"
    }
  }
}
